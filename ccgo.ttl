#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem
<http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem> rdf:type owl:ObjectProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemContent
<http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemContent> rdf:type owl:ObjectProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemOf
<http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemOf> rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/ModSem2022/ccgo/abilitàDiCarta
:abilitàDiCarta rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf [ owl:inverseOf :haCaratteristica
                                   ] ;
                owl:inverseOf :haAbilità ;
                rdfs:comment "Relazione che specifica in quali carte si trovi una determinata abilità"@it ;
                rdfs:label "abilità di"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/appartieneACollezione
:appartieneACollezione rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemOf> ,
                                          [ owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem>
                                          ] ;
                       owl:inverseOf <http://www.semanticweb.org/ModeSem2022/ccgo/collezioneContieneCarta> ;
                       rdfs:domain :CartaPosseduta ;
                       rdfs:range :CollezioneCarte ;
                       rdfs:comment "Relazione che specifica a quale Collezione appartenga una CartaPosseduta."@it ;
                       rdfs:label "inserita in Collezione"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/appartieneAMazzo
:appartieneAMazzo rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemOf> ,
                                     [ owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem>
                                     ] ;
                  owl:inverseOf <http://www.semanticweb.org/ModeSem2022/ccgo/mazzoContieneCarta> ;
                  rdfs:domain :CartaPosseduta ;
                  rdfs:range :Mazzo ;
                  rdfs:comment "Relazione che specifica a quale Mazzo appartenga una CartaPosseduta."@it ;
                  rdfs:label "inserita in Mazzo"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/cartaNonLegaleIn
:cartaNonLegaleIn rdf:type owl:ObjectProperty ;
                  rdfs:domain :Carta ;
                  rdfs:range :Formato ;
                  rdfs:comment "Proprietà che specifica che una carta non è legale in un certo formato."@it ;
                  rdfs:label "non legale in"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/costoAbilità
:costoAbilità rdf:type owl:ObjectProperty ;
              rdfs:domain :Abilità ;
              rdfs:range :Costo ;
              rdfs:comment "Relazione che specifica quale costo abbia un'abilità."@it ;
              rdfs:label "costo di"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/faRiferimentoACarta
:faRiferimentoACarta rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemContent> ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain :CartaPosseduta ;
                     rdfs:range :Carta ;
                     rdfs:comment "Relazione che specifica a quale carta faccia riferimento un oggetto CartaPosseduta."@it ;
                     rdfs:label "fa riferimento a Carta"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/faRiferimentoAGioco
:faRiferimentoAGioco rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain owl:Thing ;
                     rdfs:range <http://dbpedia.org/resource/Collectible_card_game> ,
                                :GiocoCarteCollezionabili ;
                     rdfs:comment "Relazione di appartenenza di un oggetto non GiocoCarteCollezionabili ad un dato gioco di carte collezionabili."@it ;
                     rdfs:label "appartiene a Gioco"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/haAbilità
:haAbilità rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :haCaratteristica ;
           rdfs:domain :Carta ;
           rdfs:range :Abilità ;
           rdfs:comment "Relazione che specifica quali abilità abbia una determinata carta."@it ;
           rdfs:label "ha Abilità"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/haCaratteristica
:haCaratteristica rdf:type owl:ObjectProperty ;
                  rdfs:domain :Carta ;
                  rdfs:range :CaratteristicaCarta ;
                  rdfs:comment "Proprietà che specifica una caratteristica di una carta"@it ;
                  rdfs:label "caratteristica di"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/haCosto
:haCosto rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :haCaratteristica ;
         rdfs:domain :Carta ;
         rdfs:range :Costo ;
         rdfs:comment "Relazione che specifica quale sia il costo di ua carta."@it ;
         rdfs:label "ha Costo"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/haRarità
:haRarità rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :haCaratteristica ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :Carta ;
          rdfs:range :LivelloRarità ;
          rdfs:comment "Relazione che specifica l'ultimo livello di rarità che è stato assegnato a una carta. Non tiene conto del fatto che la rarità possa essere modificata da un set all'altro, perché l'unica che conta è l'ultima assegnata."@it ;
          rdfs:label "ha Rarità"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/haSet
:haSet rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :haCaratteristica ;
       rdfs:domain :Carta ;
       rdfs:range :Set ;
       rdfs:comment "Relazione che assegna a una carta uno dei set in cui è stata stampata."@it ;
       rdfs:label "stampata in"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/interagisceCon
:interagisceCon rdf:type owl:ObjectProperty ;
                owl:inverseOf :interagisceCon ;
                rdf:type owl:SymmetricProperty ,
                         owl:TransitiveProperty ;
                rdfs:domain :Carta ;
                rdfs:range :Carta ;
                owl:propertyChainAxiom ( :haAbilità
                                         :nominaCarta
                                       ) ;
                rdfs:comment "Relazione che specifica quali carte interagiscono con una determinata carta. E' simmetrica e transitiva."@it ;
                rdfs:label "Carta interagisce con Carta"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/mazzoNonLegaleIn
:mazzoNonLegaleIn rdf:type owl:ObjectProperty ;
                  rdfs:domain :Mazzo ;
                  rdfs:range :Formato ;
                  owl:propertyChainAxiom ( <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem>
                                           <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemContent>
                                           :cartaNonLegaleIn
                                         ) ;
                  rdfs:comment "Relazione che specifica un formato in cui non il mazzo in questione non è legale."@it ;
                  rdfs:label "non legale in"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/mazzoPensatoPer
:mazzoPensatoPer rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :Mazzo ;
                 rdfs:range :Formato ;
                 rdfs:comment "Relazione che specifica in quale Formato il possessore del Mazzo intende utilizzarlo."@it ;
                 rdfs:label "pensato per"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/nominaCarta
:nominaCarta rdf:type owl:ObjectProperty ;
             rdfs:domain :Abilità ;
             rdfs:range :Carta ;
             rdfs:comment "Relazione che specifica quali carte vengono nominate nel testo di un'abilità."@it ;
             rdfs:label "nomina Carta"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/possiede
:possiede rdf:type owl:ObjectProperty ;
          rdfs:domain :Persona ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :CollezioneCarte
                                     :Mazzo
                                   )
                     ] ;
          rdfs:comment "Relazione che specifica quale Collezione o Mazzo possiede una Persona."@it ;
          rdfs:label "possiede"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/possiedeCarta
:possiedeCarta rdf:type owl:ObjectProperty ;
               rdfs:domain :Persona ;
               rdfs:range :CartaPosseduta ;
               owl:propertyChainAxiom ( :possiede
                                        <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem>
                                      ) ;
               rdfs:comment "Relazione che specifica quali oggetti CartaPosseduta appartengono a una Persona."@it ;
               rdfs:label "possiede Carta"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/setCartaPosseduta
:setCartaPosseduta rdf:type owl:ObjectProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain :CartaPosseduta ;
                   rdfs:range :Set ;
                   rdfs:comment "Proprietà che specifica di quale Set sia la carta inserita da un giocatore nel proprio mazzo o nella propria collezione"@it ;
                   rdfs:label "set"@it .


###  http://www.semanticweb.org/ModeSem2022/ccgo/collezioneContieneCarta
<http://www.semanticweb.org/ModeSem2022/ccgo/collezioneContieneCarta> rdf:type owl:ObjectProperty ;
                                                                      rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem> ,
                                                                                         [ owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemOf>
                                                                                         ] ;
                                                                      rdfs:comment "Relazione che specifica uno degli oggetti CartaPosseduta che è stato inserito in una Collezione."@it ;
                                                                      rdfs:label "contiene Carta"@it .


###  http://www.semanticweb.org/ModeSem2022/ccgo/mazzoContieneCarta
<http://www.semanticweb.org/ModeSem2022/ccgo/mazzoContieneCarta> rdf:type owl:ObjectProperty ;
                                                                 rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem> ,
                                                                                    [ owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemOf>
                                                                                    ] ;
                                                                 rdfs:comment "Relazione che specifica uno degli oggetti CartaPosseduta che è stato inserito in un Mazzo."@it ;
                                                                 rdfs:label "contiene Carta"@it .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/ModSem2022/ccgo/compostoDa
:compostoDa rdf:type owl:DatatypeProperty ;
            rdfs:domain :Costo ;
            rdfs:range xsd:string ;
            rdfs:comment "Proprietà che specifica una stringa che rappresenta il costo di un'abilità o di una carta."@it ;
            rdfs:label "composto da"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/dannoAbilita
:dannoAbilita rdf:type owl:DatatypeProperty ;
              rdfs:domain :Abilità ;
              rdfs:range xsd:integer ;
              rdfs:comment "Proprietà che specifica quanto danno infligga un'abilità"@it ;
              rdfs:label "fa danno"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/haAttacco
:haAttacco rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :CartaUnità ;
           rdfs:range xsd:integer ;
           rdfs:comment "Proprietà che specifica a quanto ammonta l'attacco di un'Unità"@it ;
           rdfs:label "ha attacco"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/haDifesa
:haDifesa rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :CartaUnità ;
          rdfs:range xsd:integer ;
          rdfs:comment "Proprietà che specifica a quanto ammonta la difesa di un'Unità."@it ;
          rdfs:label "ha difesa"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/haImmagine
:haImmagine rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :Carta ;
            rdfs:range xsd:anyURI ;
            rdfs:comment "Proprietà che specifica un URI dove è possibile visualizzare un'immagine della Carta di riferimento."@it ;
            rdfs:label "ha immagine"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/haNome
:haNome rdf:type owl:DatatypeProperty ,
                 owl:FunctionalProperty ;
        rdfs:domain owl:Thing ;
        rdfs:range xsd:string ;
        rdfs:comment "Proprietà che definisce il nome di un oggetto."@it ;
        rdfs:label "ha nome"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/haVelocita
:haVelocita rdf:type owl:DatatypeProperty ;
            rdfs:domain :CartaNonPermanente ;
            rdfs:range xsd:string ;
            rdfs:comment "Proprietà che specifica la velocità di una carta non permanente."@it ;
            rdfs:label "ha velocità"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/nomeCarta
:nomeCarta rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :haNome ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :Carta ;
           rdfs:range xsd:string ;
           rdfs:comment "Proprietà che specifica il nome di una carta all'interno del suo gioco."@it ;
           rdfs:label "Nome carta"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/nomeGioco
:nomeGioco rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :haNome ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain <http://dbpedia.org/resource/Collectible_card_game> ,
                       :GiocoCarteCollezionabili ;
           rdfs:range xsd:string ;
           rdfs:comment "Nome di un gioco di carte collezionabili"@it ;
           rdfs:label "Nome gioco"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/quantitaCarta
:quantitaCarta rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :CartaPosseduta ;
               rdfs:range xsd:integer ;
               rdfs:comment "Proprietà che specifica il numero di copie di una Carta inserita in un Mazzo o in una Collezione."@it ;
               rdfs:label "numero copie"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/sottotipoCarta
:sottotipoCarta rdf:type owl:DatatypeProperty ;
                rdfs:domain :Carta ;
                rdfs:range xsd:string ;
                rdfs:comment "Proprietà che specifica uno o più sottotipi di una determinata carta."@it ;
                rdfs:label "ha sottotipo"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/stampataPiuVolte
:stampataPiuVolte rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain :Carta ;
                  rdfs:range xsd:boolean ;
                  rdfs:comment "Proprietà che specifica se la Carta sia mai stata ristampata o meno."@it ;
                  rdfs:label "ristampa?"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/testoAbilità
:testoAbilità rdf:type owl:DatatypeProperty ;
              rdfs:domain :Abilità ;
              rdfs:range xsd:string ;
              rdfs:comment "Proprietà che specifica il testo di un'abilità."@it ;
              rdfs:label "ha testo"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/tipoCarta
:tipoCarta rdf:type owl:DatatypeProperty ;
           rdfs:domain :Carta ;
           rdfs:range xsd:string ;
           rdfs:comment "Proprietà che specifica il tipo di una carta."@it ;
           rdfs:label "ha tipo"@it .


###  http://xmlns.com/foaf/0.1/givenName
<http://xmlns.com/foaf/0.1/givenName> rdf:type owl:DatatypeProperty .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/resource/Collectible_card_game
<http://dbpedia.org/resource/Collectible_card_game> rdf:type owl:Class ;
                                                    owl:equivalentClass :GiocoCarteCollezionabili .


###  http://dbpedia.org/resource/Playing_card
<http://dbpedia.org/resource/Playing_card> rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag
<http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag> rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item
<http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item> rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> rdf:type owl:Class .


###  http://www.semanticweb.org/ModSem2022/ccgo/Abilità
:Abilità rdf:type owl:Class ;
         rdfs:subClassOf :CaratteristicaCarta ;
         rdfs:comment "Un'abilità è una capacità speciale di una carta che può alterare il normale funzionamento del gioco o influenzare altre carte in vari modi. Ogni carta può possedere un qualsiasi numero di abilità (in genere limitato solamente dalla dimensione della carta fisica/virtuale)."@it ;
         rdfs:label "Abilità carta"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/AbilitàAttivata
:AbilitàAttivata rdf:type owl:Class ;
                 rdfs:subClassOf :Abilità ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :costoAbilità ;
                                   owl:someValuesFrom :Costo
                                 ] ;
                 rdfs:comment "Abilità che può essere attivata a comando. Richiede il pagamento di un costo per l'attivazione (ma non sono le uniche abilità che possono avere un costo associato)."@it ;
                 rdfs:label "Abilità attivata"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/AbilitàInnescata
:AbilitàInnescata rdf:type owl:Class ;
                  rdfs:subClassOf :Abilità ;
                  rdfs:comment "Abilità che si innesca automaticamente al verificarsi di una determinata condizione. In genere contengono le parole \"Quando\", \"Ogniqualvolta\", ecc."@it ;
                  rdfs:label "Abilità Innescata"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/AbilitàParolaChiave
:AbilitàParolaChiave rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( :Abilità
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :haNome ;
                                                                  owl:someValuesFrom xsd:string
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:subClassOf :Abilità ;
                     owl:disjointWith :CartaPosseduta ,
                                      :Costo ;
                     rdfs:comment "Abilità di una carta rappresentata da una parola chiave. Sulle carte, generalmente, si trova solo tale parola chiave, il cui funzionamento è però descritto da un testo più lungo."@it ;
                     rdfs:label "Abilità Parola Chiave"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/AbilitàStatica
:AbilitàStatica rdf:type owl:Class ;
                rdfs:subClassOf :Abilità ;
                rdfs:comment "Abilità di una carta che ha un effetto continuo e non variabile in un certo interallo di tempo. Generalmente le abilità statiche sono legate ai permanenti, anche se non sempre è così."@it ;
                rdfs:label "Abilità statica"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/CaratteristicaCarta
:CaratteristicaCarta rdf:type owl:Class ;
                     rdfs:comment "Caratteristica o dettaglio di una specifica carta di un CCG."@it ;
                     rdfs:label "Caratteristica di una carta"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/Carta
:Carta rdf:type owl:Class ;
       rdfs:subClassOf <http://dbpedia.org/resource/Playing_card> ;
       rdfs:comment "Carta di un CCG"@it ;
       rdfs:label "Carta di un CCG."@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/CartaAusiliare
:CartaAusiliare rdf:type owl:Class ;
                rdfs:subClassOf :CartaPermanente ;
                owl:disjointWith :CartaNonPermanente ,
                                 :CartaUnità ;
                rdfs:comment "Carta permanente che non rappresenta un'unità, ma un altro tipo di carta con effetto continuo sul gioco. A seconda del CCG seguono regole diverse."@it ;
                rdfs:label "Carta ausiliare"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/CartaNonPermanente
:CartaNonPermanente rdf:type owl:Class ;
                    rdfs:subClassOf :Carta ;
                    owl:disjointWith :CartaPermanente ,
                                     :CartaUnità ;
                    rdfs:comment "Tipo di carta che esaurisce il suo effetto quando viene giocata e non rimane sul campo di gioco, ma viene scartata."@it ;
                    rdfs:label "Carta non permanente"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/CartaPermanente
:CartaPermanente rdf:type owl:Class ;
                 rdfs:subClassOf :Carta ;
                 owl:disjointUnionOf ( :CartaAusiliare
                                       :CartaUnità
                                     ) ;
                 rdfs:comment "Carta che permane sul campo di gioco dopo essere stata giocata. Le sue abilità hanno effetto fintanto che essa rimane in gioco."@it ;
                 rdfs:label "Carta permanente"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/CartaPosseduta
:CartaPosseduta rdf:type owl:Class ;
                rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item> ;
                owl:disjointWith :CollezioneCarte ,
                                 :Mazzo ;
                rdfs:comment "Una carta posseduta da una Persona che l'ha inserita o in un Mazzo o nella sua Collezione."@it ;
                rdfs:label "Carta in un Mazzo o Collezione"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/CartaUnità
:CartaUnità rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( :CartaPermanente
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :haAttacco ;
                                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                         owl:onDataRange xsd:integer
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :haDifesa ;
                                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                         owl:onDataRange xsd:integer
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            rdfs:subClassOf :CartaPermanente ;
            rdfs:comment "Carta che rappresenta un'unità. Una carta unità è sempre dotata di un attacco e di una difesa, può attaccare e se la difesa arriva a zero (per qualsiasi motivo), l'unità muore."@it ;
            rdfs:label "Carta unità"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/CollezioneCarte
:CollezioneCarte rdf:type owl:Class ;
                 rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag> ;
                 rdfs:comment "Classe che rappresenta la collezione di carte una Persona. E' unica per Persona, cioè non distingue tra le carte appartenenti a i diversi CCG."@it ;
                 rdfs:label "Collezione di carte"@it ;
                 <http://www.w3.org/2004/02/skos/core#related> <http://dbpedia.org/resource/Playing_card> .


###  http://www.semanticweb.org/ModSem2022/ccgo/Collezionista
:Collezionista rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :possiede ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass :CollezioneCarte
                                   ] ;
               rdfs:subClassOf :Persona ;
               rdfs:comment "Persona che vuole collezionare carte da uno o più CCG."@it ;
               rdfs:label "Collezionista di carte"@it ;
               <http://www.w3.org/2004/02/skos/core#related> <http://dbpedia.org/resource/Category:Collectors> .


###  http://www.semanticweb.org/ModSem2022/ccgo/Costo
:Costo rdf:type owl:Class ;
       rdfs:subClassOf :CaratteristicaCarta ;
       rdfs:comment "Costo di una carta o dell'abilità di una carta in un gioco di carte collezionabili"@it ;
       rdfs:label "Costo carta o abilità"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/Formato
:Formato rdf:type owl:Class ;
         rdfs:comment "Insieme di regole di costruzione dei mazzi che vengono applicate in ambito dei tornei sanzionati o anche del gioco tra amici per imporre limiti alla composizione dei mazzi."@it ;
         rdfs:label "Formato"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/Giocatore
:Giocatore rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Restriction ;
                                 owl:onProperty :possiede ;
                                 owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass :Mazzo
                               ] ;
           rdfs:subClassOf :Persona ;
           rdfs:comment "Persona che vuole costruire Mazzi per giocare partita di uno o più CCG."@it ;
           rdfs:label "Giocatore"@it ;
           rdfs:subclassOf <http://dbpedia.org/resource/Player_(cards)> .


###  http://www.semanticweb.org/ModSem2022/ccgo/GiocoCarteCollezionabili
:GiocoCarteCollezionabili rdf:type owl:Class ;
                          rdfs:comment "Gioco di carte collezionabili"@it ;
                          rdfs:label "Gioco di carte collezionabili"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/LivelloRarità
:LivelloRarità rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Class ;
                                     owl:oneOf ( :RaritàComune
                                                 :RaritàEpica
                                                 :RaritàMitica
                                                 :RaritàNonComune
                                                 :RaritàRara
                                               )
                                   ] ;
               rdfs:subClassOf :CaratteristicaCarta ;
               rdfs:comment "Livello di rarità di una carta. Più è alto e più la carta è difficile da trovare e potente."@it ;
               rdfs:label "Livello di rarità di una carta"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/Mazzo
:Mazzo rdf:type owl:Class ;
       rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag> ;
       rdfs:comment "Classe che rappresenta un mazzo di carte appartenenti a un solo CCG di una persona"@it ;
       rdfs:label "Mazzo personalizzato"@it ;
       <http://www.w3.org/2004/02/skos/core#related> <http://dbpedia.org/resource/Playing_card> .


###  http://www.semanticweb.org/ModSem2022/ccgo/Persona
:Persona rdf:type owl:Class ;
         rdfs:subClassOf <http://xmlns.com/foaf/0.1/Person> ;
         rdfs:comment "Persona che ha qualche interesse nei giochi di carte collezionabili"@it ;
         rdfs:label "Persona interessata ai CCG"@it .


###  http://www.semanticweb.org/ModSem2022/ccgo/Set
:Set rdf:type owl:Class ;
     rdfs:subClassOf :CaratteristicaCarta ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :haNome ;
                       owl:someValuesFrom xsd:string
                     ] ;
     rdfs:comment "Uno dei potenzialmente molteplici set in cui è stata stampata questa carta."@it ;
     rdfs:label "Set di una carta"@it .


